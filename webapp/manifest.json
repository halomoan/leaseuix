{
	"_version": "1.7.0",
	"sap.app": {
		"id": "refx.leaseuix",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZLEASEUIX_ODATA_SRV": {
				"uri": "/f30/sap/opu/odata/SAP/ZLEASEUIX_ODATA_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZLEASEUIX_ODATA_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "refx.leaseuix.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "refx.leaseuix.i18n.i18n"
				}
			},
			"": {
				"dataSource": "ZLEASEUIX_ODATA_SRV"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "refx.leaseuix.view",
				"controlId": "app",
				"controlAggregation": "pages",
				
				"async": true
			},
			"routes": [
				{
					"pattern": "contractlist",
					"name": "contractlist",
					"target": "contractlist"
				},
				{
					"pattern": "contractform",
					"name": "contractform",
					"target": "contractform"
				},
				{
					"pattern": "contractpage/{contractId}",
					"name": "contractpage",
					"target": "contractpage"
				},
				{
					"pattern": "cashflow/{contractId}/:type:/:period:",
					"name": "cashflow",
					"target": "cashflow"
				},
				{
					"pattern": "",
					"name": "manageunits",
					"target": "manageunits"
				}
			],
			"targets": {
				"contractlist": {
					"viewName": "ContractList"
				},
				"contractpage": {
					"viewName": "ContractPage"
				},
				"contractform": {
					"viewName": "ContractForm"
				},
				"cashflow": {
					"viewName": "CashFlow"
				},
				"manageunits": {
					"viewName": "ManageUnits"
				}
			}
		}
	}
}