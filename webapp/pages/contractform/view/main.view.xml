<mvc:View  xmlns:mvc="sap.ui.core.mvc" xmlns="sap.uxap" xmlns:m="sap.m" 
	controllerName="refx.leaseuix.pages.contractform.controller.main"
	xmlns:contract="refx.leaseuix.pages.contractform.block">

	<ObjectPageLayout id="ObjectPageLayout">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<heading>
					<m:Image src="https://3ljx2m1ndho3557gp169nffb-wpengine.netdna-ssl.com/wp-content/uploads/2018/02/uol-logo.png" width="auto" />
					<!--<m:Title text="{i18n>Contract}" wrapping="true"/>-->
				</heading>
				<expandedContent>
					<m:Title text="We Simplified The Contract Management Screens For You - SAPCC" titleStyle="H6"/>
				</expandedContent>

			
				<!--<snappedTitleOnMobile>-->
				<!--	<m:Title text="Header Title On Phone"/>-->
				<!--</snappedTitleOnMobile>-->
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
		
		</headerContent>
		<sections>
			<ObjectPageSection titleUppercase="false" id="section1" title="Contract">
				<subSections>
					<ObjectPageSubSection id="contract" title="Contract" titleUppercase="false">
						<blocks>
							<contract:ContractBlock1 id="blk1"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" id="section2" title="Customer">
				<subSections>
					<ObjectPageSubSection id="customer" title="Customer" titleUppercase="false">
						<blocks>
							<contract:CustomerBlock1 id="blk2"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>

</mvc:View>