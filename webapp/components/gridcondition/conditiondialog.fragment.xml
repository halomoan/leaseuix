			<Page id="formPage1" class="sapUiResponsivePadding--header" showHeader="false">
				<content>
					<core:Fragment id="form0" fragmentName="refx.leaseuix.components.gridcondition.normalform" type="XML" />
				</content>
			</Page>
			<Page id="page2" class="sapUiResponsivePadding--header" showNavButton="true" navButtonPress="onNavBack">
				<content>
					
				</content>
