<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>Customer}"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportRanges="false"
		supportMultiselect="true"
		key="BP"
		descriptionKey="Name"
	>
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="BP"
						label="BP No"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="BPID" />
						</fb:control>
					</fb:FilterGroupItem>
				
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Name"
						label="Name"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="FullName" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Email"
						label="Email"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="EmailAddress" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Address"
						label="Address"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="StreetName" />
						</fb:control>
					</fb:FilterGroupItem>
					
					
					
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>