<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>RentalUnit}"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportRanges="false"
		key="UnitId"
		descriptionKey="Name"
	>
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="UnitId"
						label="Unit ID"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="UnitId" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Name"
						label="Unit Name"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="Name" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="MainCategory"
						label="Main Category"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="MainCategory" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Category"
						label="Category"
						visibleInFilterBar="true"
					>
						<fb:control>
							<m:Input name="Category" />
						</fb:control>
					</fb:FilterGroupItem>
					
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>