<mvc:View controllerName="refx.leaseuix.controller.App" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Panel>
						<mvc:XMLView viewName="refx.leaseuix.components.selectunit.selectunit" id="selectunit" async="true"></mvc:XMLView>
					</Panel>
					<Panel>
						<HBox>
						<mvc:XMLView viewName="refx.leaseuix.components.formselcustomer.formselcustomer"   id="selectcustomer" async="true"></mvc:XMLView>
						<mvc:XMLView viewName="refx.leaseuix.components.tableselcontact.tableselcontact"  id="tableselcontact" async="true"></mvc:XMLView>
						</HBox>
					</Panel>
					<Panel>
						<mvc:XMLView viewName="refx.leaseuix.components.listselunit.listselunit" id="listselunit" async="true"></mvc:XMLView>
					</Panel>
					<Panel>
						<content>
							<Button type="Accept" text="Accept" press="getValue"></Button>
						</content>
					</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>