<mvc:View
	displayBlock="true"
	height="100%"
	xmlns="sap.f"
	xmlns:mvc="sap.ui.core.mvc"
	controllerName="refx.leaseuix.controller.ContractList">
		<FlexibleColumnLayout id="fcl" backgroundDesign="Translucent" 
			restoreFocusOnBackNavigation="true"
			 layout="{viewData>/layout}">

			<beginColumnPages>
			<mvc:XMLView id="beginView" viewName="refx.leaseuix.components.contractlst.view.contractlist"/>
		</beginColumnPages>
		<midColumnPages>
			<mvc:XMLView id="detailView" viewName="refx.leaseuix.components.contractpage.view.contractdetail"/>
		</midColumnPages>
		</FlexibleColumnLayout>
</mvc:View>