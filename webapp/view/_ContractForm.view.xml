<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.uxap" xmlns:m="sap.m" 
	xmlns:contract="refx.leaseuix.blocks.controller"
	>
	<ObjectPageLayout id="ObjectPageLayout">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<heading>
					<m:Image src="https://3ljx2m1ndho3557gp169nffb-wpengine.netdna-ssl.com/wp-content/uploads/2018/02/uol-logo.png" width="auto" />
					<!--<m:Title text="{i18n>Contract}" wrapping="true"/>-->
				</heading>
				<expandedContent>
					<m:Title text="We Simplified The Contract Management Screens For You - SAPCC" titleStyle="H6"/>
				</expandedContent>

			
				<!--<snappedTitleOnMobile>-->
				<!--	<m:Title text="Header Title On Phone"/>-->
				<!--</snappedTitleOnMobile>-->
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
		
		</headerContent>
		<sections>
			<ObjectPageSection titleUppercase="false" id="section1" title="Contract">
				<subSections>
					<ObjectPageSubSection id="contract" title="Contract" titleUppercase="false">
						<blocks>
							<contract:ContractBlock1 id="blk1"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" id="section2" title="Customer">
				<subSections>
					<ObjectPageSubSection id="customer" title="Customer" titleUppercase="false">
						<blocks>
							<contract:CustomerBlock1 id="blk2"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" id="section3" title="Condition">
				<subSections>
					<ObjectPageSubSection id="condition" title="Condition" titleUppercase="false">
						<blocks>
							<contract:ConditionBlock1 id="blk3"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" id="section4" title="Posting Parameters">
				<subSections>
					<ObjectPageSubSection id="postingParam" title="Posting Parameter" titleUppercase="false">
						<blocks>
							<contract:PostingParamBlock1 id="blk4"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
				
			</ObjectPageSection>
			<ObjectPageSection titleUppercase="false" id="section5" title="Sales Based">
				<subSections>
					<ObjectPageSubSection id="salesBased" title="Sales Based" titleUppercase="false">
						<blocks>
							<contract:SalesBasedBlock1 id="blk5"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
				
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>